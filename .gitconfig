[user]
	name = myyang
	email = ymy1019@gmail.com
[color]
	ui = true
	log = auto

[alias]
    lo = "log --graph --pretty=format:'%Cred%h%Creset %C(blue)%an%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=short"
    co = checkout
    st = status
    us = reset HEAD
    uc = reset HEAD^ --soft
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore
[grep]
	lineNumber = true
