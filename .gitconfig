[user]
	name = myyang
	email = ymy1019@gmail.com
[color]
	ui = true
	log = auto

[alias]
    cm = commit -m
	lo = log --graph --pretty=format:'%Cred%h%Creset %C(blue)%an%Creset -%C(yellow)%d%Creset %s %Cgreen(%cd)%Creset' --abbrev-commit --date=short
    co = checkout
    st = status
    us = reset HEAD
    uc = reset HEAD^ --soft
    df = diff
[core]
	editor = vim
	excludesfile = ~/.gitignore
[grep]
	lineNumber = true
[push]
	default = simple
